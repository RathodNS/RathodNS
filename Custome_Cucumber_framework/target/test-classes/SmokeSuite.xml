<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="smoke Suite" parallel="tests" thread-count="4">
	<test name="Smoke" preserve-order="true">
	<parameter name="browser" value="chrome"/>
	<parameter name="version" value="latest"/>
	<parameter name="OS" value="Windows 10" />
		<classes>
			<class name="Runner.TestRunner"/>
		</classes>
	</test>
	 <test name="Regression" preserve-order="true">
	<parameter name="browser" value="Microsoft Edge"/>
	<parameter name="OS" value="Windows 11" />
	<parameter name="version" value="latest"/>
	
		<classes>
			<class name="Runner.TestRunner"/>
		</classes>
	</test>
	<test name="Sanity" preserve-order="true">
	<parameter name="browser" value="Safari"/>
	<parameter name="version" value="16.0"/>
	<parameter name="OS" value="macOS Ventura" />
		<classes>
			<class name="Runner.TestRunner"/>
		</classes>
	</test>  
</suite>